<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="repair_order_view_tree_inherit" model="ir.ui.view">
        <field name="name">repair_order_view_tree_inherit</field>
        <field name="model">repair.order</field>
        <field name="inherit_id" ref="repair.view_repair_order_form" />
        <field name="arch" type="xml">
            <field name="description" position="before">
                <field name="vin"></field>
            </field>
            <field name="lot_id" position="replace">
                <field name="mileage"></field>
                <field name="custom_lot_id"></field>
            </field>
        </field>
    </record>
</odoo>